{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "tsc && vite build",
    "build:server": "tsc --project server/tsconfig.json",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "test": "vitest",
    "db:init": "tsx server/scripts/initDb.ts",
    "db:seed": "tsx server/scripts/seedSampleProducts.ts",
    "db:seed:categories": "tsx server/scripts/seedCategories.ts",
    "db:test:auth": "tsx server/scripts/testAuth.ts"
  },
  "dependencies": {
    "@sentry/browser": "^10.10.0",
    "@sentry/react": "^10.10.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/recharts": "^2.0.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "framer-motion": "^12.23.6",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "logrocket": "^10.1.0",
    "lucide-react": "^0.511.0",
    "pg": "^8.16.3",
    "razorpay": "^2.9.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "recharts": "^3.1.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/express": "^5.0.4",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.27.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "postcss": "^8.4.35",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vite-plugin-image-optimizer": "^2.0.2",
    "vitest": "^3.2.4"
  }
}